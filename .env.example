# Environment Variables Template
# Copy this file to .env and fill in the values

# Grants.gov API endpoint for scanning opportunities
GRANTS_API_ENDPOINT=https://www.grants.gov/grantsws/rest/opportunities/search/

# Database location (optional - defaults to tracker.db)
DATABASE_PATH=tracker.db

# CSV output directory (optional - defaults to current directory)
OUTPUT_DIR=./

# Evidence collection directory (optional - defaults to data/denials/)
EVIDENCE_DIR=data/denials/